   "source": [
    "# List of models to evaluate\n",
    "models_to_test = [\n",
    "    \"us.anthropic.claude-3-7-sonnet-20250219-v1:0\",\n",
    "    \"us.amazon.nova-pro-v1:0\",\n",
    "    \"us.anthropic.claude-3-5-sonnet-20241022-v2:0\",\n",
    "    \"us.anthropic.claude-3-5-haiku-20241022-v1:0\"\n",
    "]\n",
    "\n",
    "# Store results for comparison\n",
    "model_results = []\n",
    "\n",
    "# Test each model with the same text\n",
    "for model_id in models_to_test:\n",
    "    print(f\"\\n\\n{'='*80}\")\n",
    "    print(f\"TESTING MODEL: {model_id}\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    # Update the model in the config\n",
    "    test_config = CONFIG.copy()\n",
    "    test_config[\"summarization\"][\"model\"] = model_id\n",
    "    \n",
    "    # Create summarization service with the test model\n",
    "    test_service = summarization.SummarizationService(\n",
    "        config=test_config, \n",
    "        backend=\"bedrock\"\n",
    "    )\n",
    "    \n",
    "    # Summarize the text\n",
    "    print(f\"Summarizing with {model_id}...\")\n",
    "    start_time = time.time()\n",
    "    summary = test_service.summarize_text(sample_text)\n",
    "    model_time = time.time() - start_time\n",
    "    \n",
    "    print(f\"Summarization completed in {model_time:.2f} seconds\")\n",
    "    \n",
    "    # Display the summary\n",
    "    print(\"\\nBrief Summary:\")\n",
    "    print(summary.brief_summary)\n",
    "    \n",
    "    print(\"\\nDetailed Summary:\")\n",
    "    print(summary.detailed_summary[:300] + \"...\" if len(summary.detailed_summary) > 300 else summary.detailed_summary)\n",
    "    \n",
    "    # Store results\n",
    "    model_results.append({\n",
    "        \"model\": model_id,\n",
    "        \"time\": model_time,\n",
    "        \"brief_summary\": summary.brief_summary,\n",
    "        \"detailed_summary\": summary.detailed_summary,\n",
    "        \"brief_length\": len(summary.brief_summary),\n",
    "        \"detailed_length\": len(summary.detailed_summary)\n",
    "    })\n",
    "\n",
    "# Print summary of results\n",
    "print(\"\\n\\n\" + \"=\"*80)\n",
    "print(\"MODEL COMPARISON SUMMARY\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "for result in model_results:\n",
    "    print(f\"\\nModel: {result['model']}\")\n",
    "    print(f\"  Processing time: {result['time']:.2f} seconds\")\n",
    "    print(f\"  Brief summary length: {result['brief_length']} characters\")\n",
    "    print(f\"  Detailed summary length: {result['detailed_length']} characters\")"
   ]
