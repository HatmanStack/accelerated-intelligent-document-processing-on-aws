{
    "StartAt": "TextractStep",
    "States": {
      "TextractStep": {
        "Type": "Task",
        "Resource": "${TextractFunctionArn}",
        "ResultPath": "$.textractResult",
        "Next": "BedrockStep"
      },
      "BedrockStep": {
        "Type": "Task",
        "Resource": "${BedrockFunctionArn}",
        "Parameters": {
          "text.$": "$.textractResult",
          "outputBucket": "${OutputBucket}",
          "detail": {
            "object": {
              "key.$": "$.detail.object.key"
            }
          }
        },
        "End": true
      }
    }
}